    Private Sub ToolStripButton1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ToolStripButton1.Click
        If Transcript.Text <> "" Then
            Clipboard.Clear()
            Dim data_object As New DataObject
            Transcript.SelectAll()
            Transcript.SelectionAlignment() = HorizontalAlignment.Left
            data_object.SetData(DataFormats.Rtf, Transcript.Rtf)
            data_object.SetData(DataFormats.UnicodeText, Transcript.Text)
            Clipboard.SetDataObject(data_object)
            Transcript.SelectAll()
            Transcript.SelectionAlignment() = HorizontalAlignment.Center
            Transcript.DeselectAll()
            Transcript.SelectionStart = Transcript.TextLength
            Transcript.SelectionLength = 1
        Else
            MsgBox("Transcription is empty.", MsgBoxStyle.OkOnly, "Copy Error")
        End If
    End Sub

